    "argon2": "^0.31.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.9.0",
    "crypto": "^1.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.2",
    "hono": "^3.10.1",
    "jsonwebtoken": "^9.0.2",
    "mock-aws-s3": "^4.0.2",
    "nock": "^14.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.1",
